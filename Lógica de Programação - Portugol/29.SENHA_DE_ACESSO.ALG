algoritmo "SENHA_DE_ACESSO"

var
   Num : inteiro

inicio
      Escreval ("###############  SENHA   ###############")
      Num <- 1
      Enquanto (num <= 100) faca
      Escreval ("SENHA NÚMERO: ", Num, "")
      Num <- Num+1
      Fimenquanto
      Escreval ("ACABOU AS SENHAS DO DIA !!!")

fimalgoritmo